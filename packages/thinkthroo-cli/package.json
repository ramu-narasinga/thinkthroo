{
  "name": "@thinkthroo/cli",
  "version": "1.0.0",
  "description": "Add features such as authentication, file upload via CLI and save time.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "default": "./dist/registry/index.js"
    }
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start:dev": "cross-env REGISTRY_URL=http://localhost:3000/r node dist/index.js",
    "start:prod": "cross-env REGISTRY_URL=https://ui.shadcn.com/r node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramu-narasinga/thinkthroo.git"
  },
  "keywords": [
    "auth",
    "file-upload",
    "cli",
    "nextjs",
    "thinkthroo"
  ],
  "author": "Ramu Narasinga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramu-narasinga/thinkthroo/issues"
  },
  "homepage": "https://github.com/ramu-narasinga/thinkthroo#readme",
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/prompts": "^2.4.9"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "tsup": "^8.4.0",
    "zod": "^3.24.2"
  }
}
