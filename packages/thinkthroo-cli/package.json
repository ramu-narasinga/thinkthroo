{
  "name": "@thinkthroo/cli",
  "version": "1.0.0",
  "description": "Add features such as authentication, file upload via CLI and save time.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "default": "./dist/registry/index.js"
    }
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramu-narasinga/thinkthroo.git"
  },
  "keywords": [
    "auth",
    "file-upload",
    "cli",
    "nextjs",
    "thinkthroo"
  ],
  "author": "Ramu Narasinga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramu-narasinga/thinkthroo/issues"
  },
  "homepage": "https://github.com/ramu-narasinga/thinkthroo#readme",
  "devDependencies": {
    "@repo/typescript-config": "*"
  },
  "dependencies": {
    "tsup": "^8.4.0"
  }
}
